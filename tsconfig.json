{
  "compilerOptions": {
    "target": "es6",                          
    "module": "commonjs",
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "jsx": "react",
  }
}
